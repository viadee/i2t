    </xs:complexType>
  </xs:element>
  <!-- A worklet is a reusable workflow, which can be used within a workflow -->
  <!-- worklet can not be used as stand alone object -->
  <xs:element name="WORKLET">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="TASK"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SESSION"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WORKLET"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="TASKINSTANCE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WORKFLOWLINK"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WORKFLOWVARIABLE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WORKFLOWEVENT"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ATTRIBUTE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="METADATAEXTENSION"/>
      </xs:sequence>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="DESCRIPTION"/>
      <xs:attribute name="REUSABLE" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="ISVALID">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="PARENT"/>
      <xs:attribute name="PARENT_TYPE">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="WORKLET"/>
            <xs:enumeration value="WORKFLOW"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="VERSIONNUMBER"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ATTRIBUTE">
    <xs:complexType>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="VALUE" use="required"/>
      <xs:attribute name="PARTITIONNAME"/>
    </xs:complexType>
  </xs:element>
  <!-- Sessions can be reusable or non-reusable -->
  <xs:element name="SESSION">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="TASK"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SESSTRANSFORMATIONINST"/>
        <xs:element minOccurs="0" ref="CONFIGREFERENCE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SESSIONCOMPONENT"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SESSIONEXTENSION"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ATTRIBUTE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="METADATAEXTENSION"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="RESOURCEREFERENCE"/>
          <xs:element ref="TRANSFORMRESOURCEREF"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="DESCRIPTION"/>
      <xs:attribute name="REUSABLE" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="ISVALID">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="MAPPINGNAME" use="required"/>
      <xs:attribute name="MAPPINGVERSION"/>
      <xs:attribute name="SORTORDER"/>
      <xs:attribute name="PARENT"/>
      <xs:attribute name="PARENT_TYPE">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="WORKLET"/>
            <xs:enumeration value="WORKFLOW"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="VERSIONNUMBER"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="CONFIGREFERENCE">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ATTRIBUTE"/>
      </xs:sequence>
      <xs:attribute name="REFOBJECTNAME" use="required"/>
      <xs:attribute name="TYPE" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- Sinstance identifies the usage and overriden attributes of a transformation instance used within a session task -->
  <xs:element name="SESSTRANSFORMATIONINST">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SESSTRANSFORMATIONGROUP"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="PARTITION"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="HASHKEY"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="FLATFILE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ATTRIBUTE"/>
      </xs:sequence>
      <xs:attribute name="SINSTANCENAME" use="required"/>
      <xs:attribute name="TRANSFORMATIONNAME" use="required"/>
      <xs:attribute name="TRANSFORMATIONTYPE" use="required"/>
      <xs:attribute name="PARTITIONTYPE"/>
      <xs:attribute name="ISREPARTITIONPOINT">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="STAGE"/>
      <xs:attribute name="PIPELINE"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="SESSTRANSFORMATIONGROUP">
    <xs:complexType>
      <xs:attribute name="GROUP" use="required"/>
      <xs:attribute name="PARTITIONTYPE"/>
    </xs:complexType>
  </xs:element>
  <!-- Hash keys are associated with a session instance -->
  <xs:element name="HASHKEY">
    <xs:complexType>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="GROUP"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PARTITION">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="KEYRANGE"/>
      </xs:sequence>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="DESCRIPTION"/>
    </xs:complexType>
  </xs:element>
  <!-- Key ranges associated with the partition -->
  <xs:element name="KEYRANGE">
    <xs:complexType>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="STARTRANGE"/>
      <xs:attribute name="ENDRANGE"/>
      <xs:attribute name="SRCINSTANCENAME"/>
      <xs:attribute name="GROUP"/>
    </xs:complexType>
  </xs:element>
  <!-- Config defines a list session properties, it can be reusable or non reusable -->
  <xs:element name="CONFIG">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ATTRIBUTE"/>
      </xs:sequence>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="DESCRIPTION"/>
      <xs:attribute name="ISDEFAULT">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="VERSIONNUMBER"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="SESSIONCOMPONENT">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="TASK"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="VALUEPAIR"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ATTRIBUTE"/>
      </xs:sequence>
      <xs:attribute name="TYPE" use="required"/>
      <xs:attribute name="REFOBJECTNAME" use="required"/>
      <xs:attribute name="REUSABLE" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="ISNONE">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="CONNECTIONREFERENCE">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ATTRIBUTE"/>
      </xs:sequence>
      <xs:attribute name="CNXREFNAME" use="required"/>
      <xs:attribute name="CONNECTIONTYPE" use="required"/>
      <xs:attribute name="CONNECTIONSUBTYPE" use="required"/>
      <xs:attribute name="CONNECTIONNUMBER" use="required"/>
      <xs:attribute name="CONNECTIONNAME" use="required"/>
      <xs:attribute name="PARTITIONNAME"/>
      <xs:attribute name="VARIABLE"/>
      <xs:attribute name="COMPONENTVERSION"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="SESSIONEXTENSION">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="CONNECTIONREFERENCE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ATTRIBUTE"/>
      </xs:sequence>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="TYPE" use="required"/>
      <xs:attribute name="SUBTYPE" use="required"/>
      <xs:attribute name="SINSTANCENAME" use="required"/>
      <xs:attribute name="TRANSFORMATIONTYPE" use="required"/>
      <xs:attribute name="DSQINSTNAME"/>
      <xs:attribute name="DSQINSTTYPE"/>
      <xs:attribute name="COMPONENTVERSION"/>
    </xs:complexType>
  </xs:element>
  <!--
    Specifies a resource required by a session/command task instance, 
    or a transformation in session instance
  -->
  <xs:element name="RESOURCEREFERENCE">
    <xs:complexType>
      <xs:attribute name="TYPE" use="required"/>
      <xs:attribute name="NAME" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TRANSFORMRESOURCEREF">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="RESOURCEREFERENCE"/>
      </xs:sequence>
      <xs:attribute name="SINSTANCENAME" use="required"/>
      <xs:attribute name="TRANSFORMATIONTYPE" use="required"/>
    </xs:complexType>
  </xs:element>
  <!--
    A workflow is a set of actions & rules on how and when to execute those actions. 
    every action can be defined as an instance of an executable object 
    workflow consists of reusable & non-reusable object instances
  -->
  <xs:element name="WORKFLOW">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="SCHEDULER"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="TASK"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SESSION"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WORKLET"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="TASKINSTANCE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WORKFLOWLINK"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WORKFLOWVARIABLE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WORKFLOWEVENT"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ATTRIBUTE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="METADATAEXTENSION"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="CONWFRUNINFO"/>
      </xs:sequence>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="DESCRIPTION"/>
      <xs:attribute name="SERVERNAME"/>
      <xs:attribute name="SERVER_DOMAINNAME"/>
      <xs:attribute name="SUSPENSION_EMAIL"/>
      <xs:attribute name="SCHEDULERNAME"/>
      <xs:attribute name="REUSABLE_SCHEDULER">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="ISVALID">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="ISENABLED">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="SUSPEND_ON_ERROR">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="TASKS_MUST_RUN_ON_SERVER">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="VERSIONNUMBER"/>
      <xs:attribute name="ISSERVICE">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="ISRUNNABLESERVICE">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="SERVICELEVELNAME"/>
    </xs:complexType>
  </xs:element>
  <!--
    Instance of the task used within a workflow instance has attributes overriden ;
    if the instance is of type session it has additional overrides as such SESSIONCOMPONENTs 
    and sessions
  -->
  <xs:element name="TASKINSTANCE">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ATTRIBUTE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WORKFLOWVARIABLE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="VALUEPAIR"/>
        <xs:element minOccurs="0" ref="CONFIGREFERENCE"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SESSIONCOMPONENT"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SESSIONEXTENSION"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="RESOURCEREFERENCE"/>
          <xs:element ref="TRANSFORMRESOURCEREF"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="DESCRIPTION"/>
      <xs:attribute name="TASKTYPE" use="required"/>
      <xs:attribute name="TASKNAME" use="required"/>
      <xs:attribute name="ISENABLED">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="REUSABLE" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="TREAT_INPUTLINK_AS_AND">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="FAIL_PARENT_IF_INSTANCE_FAILS">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="FAIL_PARENT_IF_INSTANCE_DID_NOT_RUN">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="SERVER_OVERRIDE"/>
    </xs:complexType>
  </xs:element>
  <!-- A link represents the flow of execution from one object instance to the other -->
  <xs:element name="WORKFLOWLINK">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="EXPRMACRODEPENDENCY"/>
      </xs:sequence>
      <xs:attribute name="FROMTASK" use="required"/>
      <xs:attribute name="TOTASK" use="required"/>
      <xs:attribute name="CONDITION"/>
    </xs:complexType>
  </xs:element>
  <!-- Variables are placeholders for recording information like status of an object -->
  <xs:element name="WORKFLOWVARIABLE">
    <xs:complexType>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="DESCRIPTION"/>
      <xs:attribute name="USERDEFINED" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="DATATYPE"/>
      <xs:attribute name="DEFAULTVALUE"/>
      <xs:attribute name="ISPERSISTENT">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="ISNULL">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- An event is a notification that occurs in response to an action. users may want to -->
  <!-- start the execution of an object instance based on an event -->
  <xs:element name="WORKFLOWEVENT">
    <xs:complexType>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="DESCRIPTION"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="EXPRMACRO">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="MACROARGUMENT"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="MACRODEPENDENCY"/>
      </xs:sequence>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="DESCRIPTION"/>
      <xs:attribute name="OBJECTVERSION"/>
      <xs:attribute name="PROTOTYPE"/>
      <xs:attribute name="EXPRESSION" use="required"/>
      <xs:attribute name="MACROTYPE">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Public"/>
            <xs:enumeration value="Private"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="RETURNTYPE"/>
      <xs:attribute name="VERSIONNUMBER"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="MACROARGUMENT">
    <xs:complexType>
      <xs:attribute name="NAME" use="required"/>
      <xs:attribute name="DATATYPE"/>
      <xs:attribute name="PRECISION"/>
      <xs:attribute name="SCALE"/>
      <xs:attribute name="ARGORDER"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="MACRODEPENDENCY">
    <xs:complexType>
      <xs:attribute name="NAME" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="EXPRMACRODEPENDENCY">
    <xs:complexType>
      <xs:attribute name="NAME" use="required"/>
    </xs:complexType>
  </xs:element>
</xs:schema>
